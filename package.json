{
  "name": "@skripio/response-emitter",
  "version": "1.0.2",
  "description": "Library to unify communication with 1C:Enterprise platform.",
  "main": "index.js",
  "scripts": {
    "release-test": "standard-version -a --dry-run",
    "release-it": "standard-version -a",
    "document-it": "documentation readme ./index.js -s=\"Reference\" --markdown-toc-max-depth=3 --sort-order=alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmecgroup/skripio-helper-response-emitter.git"
  },
  "keywords": [
    "1c",
    "skripio",
    "fmec"
  ],
  "author": "Andrey Skripchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fmecgroup/skripio-helper-response-emitter/issues"
  },
  "homepage": "https://github.com/fmecgroup/skripio-helper-response-emitter#readme",
  "dependencies": {},
  "devDependencies": {
    "documentation": "^13.2.5",
    "semistandard": "^16.0.1",
    "standard-version": "^9.3.1"
  },
  "standard.options": {
    "ignore": [
      "node_modules/**"
    ]
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run document-it && git add ./README.md"
    },
    "issueUrlFormat": "https://dev.azure.com/fmec/FMS/_workitems/edit/{{id}}"
  }
}
